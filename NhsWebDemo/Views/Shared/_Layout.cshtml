<!DOCTYPE html>
<html lang="en">
<head>
         <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>@ViewData["Title"] - NhsWebDemo</title>
@*    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/NhsWebDemo.styles.css" asp-append-version="true" />
        <!-- Custom styles for this template -->
    <link href="~/css/styles.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js" crossorigin="anonymous"></script>
    <!--Jquery Datatables-->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs5/jszip-2.5.0/dt-1.11.4/b-2.2.2/b-colvis-2.2.2/b-html5-2.2.2/fh-3.2.1/sp-1.4.0/sl-1.3.4/datatables.min.css"/>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/responsive/2.2.9/css/responsive.dataTables.min.css"/>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.4/css/dataTables.bootstrap5.min.css"/>

</head>
     <body class="sb-nav-fixed">
        <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
            <!-- Navbar Brand-->
            <a class="navbar-brand ps-3" asp-area="" asp-controller="Home" asp-action="Index">Nhs Web Demo</a>
            <!-- Sidebar Toggle-->
            <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
            <!-- Navbar Search-->
            <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
                <div class="input-group">
                    <input class="form-control" type="text" placeholder="Search for..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                    <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
                </div>
            </form>
            <!-- Navbar-->
            <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                    <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                        <li><a class="dropdown-item" href="#!">Settings</a></li>
                        <li><a class="dropdown-item" href="#!">Activity Log</a></li>
                        <li><hr class="dropdown-divider" /></li>
                        <li><a class="dropdown-item" href="#!">Logout</a></li>
                    </ul>
                </li>
            </ul>
        </nav>
        <div id="layoutSidenav">
            <div id="layoutSidenav_nav">
                <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                    <div class="sb-sidenav-menu">
                        <div class="nav">
                            <!--Home Section-->
                            <div class="sb-sidenav-menu-heading">home</div>
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-home"></i></div>
                                Index
                            </a>
                            <!--Call Center Section-->
                            <div class="sb-sidenav-menu-heading">Call Center</div>
                            <a class="nav-link" asp-area="" asp-controller="CallCenter" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-home"></i></div>
                                Index
                            </a>
                            <a class="nav-link" asp-area="" asp-controller="CallCenter" asp-action="Dashboard">
                                <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                                Dashboard
                            </a>
                            <!--NHS Section-->
                            <div class="sb-sidenav-menu-heading">NHS</div>
                            <a class="nav-link" asp-area="" asp-controller="NHS" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-home"></i></div>
                                Index
                            </a>
                            <a class="nav-link" asp-area="" asp-controller="Local" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-street-view"></i></div>
                                Local
                            </a>
                            <a class="nav-link" asp-area="" asp-controller="Region" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-map-marked-alt"></i></div>
                                Regional
                            </a>
                            <a class="nav-link" asp-area="" asp-controller="National" asp-action="Index">
                                <div class="sb-nav-link-icon"><i class="fas fa-flag"></i></div>
                                National
                            </a>
                           <!--Next Section-->
                            <div class="sb-sidenav-menu-heading">Mobile App</div>
                            <a class="nav-link" asp-area="" asp-controller="MobileApp" asp-action="About">
                                <div class="sb-nav-link-icon"><i class="fas fa-mobile-alt"></i></div>
                                About
                            </a>
                        </div>
                    </div>
                    <div class="sb-sidenav-footer">
                        <div class="small">Logged in as:</div>
                        Some Person
                    </div>
                </nav>
            </div>
            <div id="layoutSidenav_content">
                <main>
                    @RenderBody()
                </main>
                <footer class="py-4 bg-light mt-auto">
                    <div class="container-fluid px-4">
                        <div class="d-flex align-items-center justify-content-between small">
                            <div class="text-muted">Copyright &copy; Andy Donegan 2022 &middot;
                                <a  asp-area="" asp-controller="Home" asp-action="Privacy">Privacy Policy</a>
                            </div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>    


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/js/sidebar.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>

    <script src="https://cdn.datatables.net/1.11.4/js/dataTables.bootstrap5.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>


</html>
